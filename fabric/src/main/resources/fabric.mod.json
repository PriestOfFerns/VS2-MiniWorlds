{
  "schemaVersion": 1,
  "id": "vs_miniworlds",
  "version": "${version}",
  "name": "VS Mini Worlds",
  "description": "A VS2 addon, that uses static VS2 ships to make small worlds for you to enjoy",
  "authors": [
    "PriestOfFern"
  ],
  "contact": {
    "homepage": "https://valkyrienskies.org/",
    "sources": "https://github.com/ValkyrienSkies/Eureka"
  },
  "license": "Apache-2.0",
  "icon": "assets/vs_miniworlds/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.priestoffern.vs_miniworlds.fabric.VSAddonTemplateModFabric"
    ],
    "client": [
      "org.priestoffern.vs_miniworlds.fabric.VSAddonTemplateModFabric${'$'}Client"
    ],
    "preLaunch": [
      "org.priestoffern.vs_miniworlds.fabric.ValkyrienPreLaunch"
    ]
  },
  "mixins": [
    "vs_addon_template-common.mixins.json",
    "vs_addon_template.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.18.2",
    "valkyrienskies": ">=${vs2_version}",
    "fabricloader": ">=0.14.21"
  }
}